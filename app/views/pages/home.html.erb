<h1>Project Management App</h1>
<% if user_signed_in? %>
  <h3>List of Org Projects</h3>
  <% if @projects.count == 0 %>
    <p>No Projects to Display </p>
  <% else %>
    <ul class="list-group">
    <% @projects.map do |project| %>
      <li class="list-group-item text-dark"><%= link_to project.name, project, class:"text-dark text-decoration-none" %></li>

    <% end %>
    </ul>
  <% end %>
  <h3>List of Org's Employee </h3>
  <% if @members.count == 0 %>
    <p>No Members to Display </p>
  <% else %>
    <ul class="list-group">
    <% @members.map do |member| %>
      <li class="list-group-item"><%= member.email %>
        
        </td></li>
    <% end %>
    </ul>
  <% end %>
<% else %>
